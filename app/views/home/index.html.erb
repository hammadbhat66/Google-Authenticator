<script type="text/javascript">
	$("#user-email").keyup(function(){
		var email_val = $("#user-email").val();
		$.post("https://multipleuser.herokuapp.com"+<%=otp_required_path%>,
			{
				email: email_val
			}
			);
	});
</script>
