<div class="container hom-ind-con1">
	<div class="row">
		<div class="hom-ind-par1 col-6">
			<%= render "devise/shared/new_session"%>
		</div>
		<div class="hom-ind-par2 col-6">
			<%= render "devise/shared/new_registrations"%>
		</div>
